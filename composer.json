{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "doctrine/dbal": "^4.3",
    "hidehalo/nanoid-php": "^2.0",
    "inertiajs/inertia-laravel": "^2.0",
    "laravel/breeze": "^2.0",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.10",
    "league/flysystem-aws-s3-v3": "^3.29",
    "minishlink/web-push": "^9.0",
    "propaganistas/laravel-phone": "^6.0",
    "simplesoftwareio/simple-qrcode": "^4.2",
    "spatie/laravel-medialibrary": "^11.7",
    "spatie/laravel-permission": "^6.17",
    "spatie/laravel-query-builder": "^6.0",
    "teamtnt/laravel-scout-tntsearch-driver": "^15.0",
    "tightenco/ziggy": "^2.5.2",
    "tymon/jwt-auth": "^2.2.1"
  },
  "require-dev": {
    "fakerphp/faker": "^1.24",
    "friendsofphp/php-cs-fixer": "^3.75",
    "ladumor/laravel-pwa": "^0.0.4",
    "laradumps/laradumps": "^4.5",
    "laradumps/laradumps-core": "^3.2",
    "laravel/boost": "^1.0",
    "laravel/pint": "^1.22",
    "laravel/sail": "^1.41",
    "mockery/mockery": "^1.6",
    "pestphp/pest": "^3.8.1",
    "pestphp/pest-plugin-laravel": "^3.1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
